{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "react/prop-types": 0,
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "import/prefer-default-export": "off",
      "react/forbid-prop-types": "off",
      "no-alert": "off"
    },
    "plugins": [
      "react",
      "jsx-a11y",
      "import"
    ],
    "env": {
      "browser": true
    }
  }